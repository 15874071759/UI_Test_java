<?xml version="1.0" encoding="UTF-8"?>
<page name="loginpage">
	    <element name="用户名输入框" type="css" value="input#account" timeout="5">usernameInputbox</element>
	    <element name="密码输入框" type="css" value="input[name='password']" timeout="5">passwordInputbox</element>
	    <element name="保持登录" type="css" value="input[value='on']" timeout="5">keepLogin</element>
	    <element name="登录按钮" type="css" value="button#submit" timeout="5">loginButton</element>
	    <element name="忘记密码" type="css" value="a[href='/zentao/user-reset.html']" timeout="5">forgetPasswd</element>
	    <element name="语言设置按钮" type="css" value="div#langs>button.btn" timeout="5">languageButton</element>
	    <element name="切换英语链接" type="css" value="div#langs>ul li:nth-child(3)" timeout="5">englishLink</element>
	    <element name="切换简体链接" type="css" value="div#langs>ul li:nth-child(1)" timeout="5">schineseLink</element>
	    <element name="切换繁体链接" type="css" value="div#langs>ul li:nth-child(2)" timeout="5">chineseLink</element>
	    <element name="手机扫描" type="css" value="button#mobile" timeout="5">phone</element>
</page>